{
  "name": "overtabbed-browser-extension",
  "version": "1.0.0",
  "description": "A Chrome extension to manage all tabs in a convenient and visual way",
  "type": "module",
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "dev": "NODE_ENV=development vite build --watch --mode development",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "generate-icons": "tsx scripts/generate-icons.ts",
    "codegen": "tsx scripts/codegen.ts"
  },
  "keywords": [
    "chrome-extension",
    "tab-manager",
    "browser-extension"
  ],
  "author": "lx223",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "@bufbuild/protobuf": "^2.1.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.36.0",
    "@bufbuild/protoc-gen-es": "^2.1.0",
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/chrome": "^0.1.29",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.15.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.11",
    "hot-reload-extension-vite": "^1.1.0",
    "jsdom": "^26.0.0",
    "sharp": "^0.34.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.2.2",
    "vitest": "^3.2.3"
  }
}